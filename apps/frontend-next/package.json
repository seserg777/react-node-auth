    "react-dom": "^18.2.0",
    "react-redux": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.0",
    "typescript": "^5"
  }
